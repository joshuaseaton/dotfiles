[
    {
        "key": "install python language server",
        "val": "pip3 list | grep python-language-server > /dev/null 2>&1 || pip3 install python-language-server"
    },
    {
        "key": "get go language server",
        "val": "go get golang.org/x/tools/cmd/gopls"
    },
    {
        "key": "build go language server",
        "val": "test -f $HOME/go/bin/gopls || go build -o $HOME/go/bin/gopls $HOME/go/src/golang.org/x/tools/cmd/gopls"
    }
]